  content: &quot;&quot;;
  position: absolute;
  z-index: -2;
  inset: -5px;
  transform: translate(10px, 8px);
  background: qlineargradient(x1:0, y1:0, x2:1, y2:0, stop: 0 rgb(198,198,200), stop: 1 rgb(204,204,206));
  filter: blur(10px);
}

QLineEdit::after {
  content: &quot;&quot;;
  position: absolute;
  z-index: -1;
  inset: 0;
  background: inherit;
  border: inherit;
  box-shadow: inherit;
}</string>
